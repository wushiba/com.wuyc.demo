<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yfshop.code.mapper.TraceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yfshop.code.model.Trace">
        <id column="id" property="id" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="trace_no" property="traceNo" />
        <result column="box_no" property="boxNo" />
        <result column="product_no" property="productNo" />
        <result column="dealer_no" property="dealerNo" />
        <result column="dealer_name" property="dealerName" />
        <result column="dealer_address" property="dealerAddress" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, create_time, update_time, trace_no, box_no, product_no, dealer_no, dealer_name, dealer_address
    </sql>

</mapper>
