<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yfshop.code.mapper.TraceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yfshop.code.model.Trace">
        <id column="id" property="id" />
        <result column="create_time" property="createTime" />
        <result column="trace_no" property="traceNo" />
        <result column="box_no" property="boxNo" />
        <result column="product_no" property="productNo" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, create_time, trace_no, box_no, product_no
    </sql>

</mapper>
