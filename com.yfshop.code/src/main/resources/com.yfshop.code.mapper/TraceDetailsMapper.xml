<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yfshop.code.mapper.TraceDetailsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yfshop.code.model.TraceDetails">
        <id column="id" property="id" />
        <result column="create_time" property="createTime" />
        <result column="box_no" property="boxNo" />
        <result column="dealer_no" property="dealerNo" />
        <result column="dealer_mobile" property="dealerMobile" />
        <result column="dealer_name" property="dealerName" />
        <result column="dealer_address" property="dealerAddress" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, create_time, box_no, dealer_no, dealer_mobile, dealer_name, dealer_address
    </sql>

</mapper>
