<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yfshop.code.mapper.MenuMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yfshop.code.model.Menu">
        <id column="id" property="id" />
        <result column="parent_menu_alias" property="parentMenuAlias" />
        <result column="menu_name" property="menuName" />
        <result column="menu_alias" property="menuAlias" />
        <result column="menu_icon" property="menuIcon" />
        <result column="link_url" property="linkUrl" />
        <result column="role_alias" property="roleAlias" />
        <result column="sort" property="sort" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, parent_menu_alias, menu_name, menu_alias, menu_icon, link_url, role_alias, sort
    </sql>

</mapper>
